package MovieClassLayer;

import java.util.List;

public class Director {

	public List<Director> DirectorID;
	public String DirectorName;
	public Director(List<Director> directorID, String directorName)
	{
		this.DirectorID= directorID;
		this.DirectorName= directorName;
	}

	public Director(){

		this.DirectorID= null;
		this.DirectorName= null;
	}
	
	
	public List<Director> getDirectorID() {
		return DirectorID;
	}

	public void setDirectorID(List<Director>  directorID) {
		DirectorID = directorID;
	}

	public String getDirectorName() {
		return DirectorName;
	}

	public void setDirectorName(String directorName) {
		DirectorName = directorName;
	}
}